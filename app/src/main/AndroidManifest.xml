<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.johnlennonlobo.appilunne" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppIlunne">
        <activity
            android:name=".ui.activity.main_home.MainHomeActivity"
            android:label="@string/title_activity_main_home"
            android:theme="@style/Theme.AppIlunne.NoActionBar" >
        </activity>

        <activity
            android:name="com.johnlennonlobo.appilunne.ui.activity.login.LoginActivity"/>

        <activity
            android:name="com.johnlennonlobo.appilunne.ui.activity.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.johnlennonlobo.appilunne.ui.activity.main.MainActivity" />
    </application>

</manifest>